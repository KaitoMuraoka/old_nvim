{
        "diagnostic.enable": false,
        "python.linting.enabled": false,
        "python.jediPath": "~/.pyenv/versions/neovim3/lib/python3.7/site-packages/",
         // add HTML and CSS
             "html-css-support.enabledLanguages": [
        "html",
        "vue",
        "blade",
        "htmldjango",
        "typescriptreact",
        "javascriptreact"
    ],
    "html-css-support.styleSheets": [
        "https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css",
        "/style.css",
        "style.css"
    ],
    // Vue.js
    "eslint.autoFixOnSave": true,
    "eslint.filetype": [
      "javascript",
      "javascriptreact",
      "typescript",
      "typescriptreact",
      "vue"
    ]

    // Swift
      "suggest.enablePreselect": true,
  "languageserver": {
    "swift": {
      "command": "/usr/bin/sourcekit-lsp",
      "filetypes": ["swift"]
    }
  },
  "java.imports.gradle.wrapper.checksums": [
    {
      "sha256": "e2b82129ab64751fd40437007bd2f7f2afb3c6e41a9198e628650b22d5824a14",
      "allowed": false
    }
  ]
}
